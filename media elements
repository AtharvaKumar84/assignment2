    <!-- Audio -->
    <audio controls>
        <source src="your-audio-file.mp3" type="audio/mp3">
        
    </audio>

    <video width="600" height="400" controls>
        <source src="your-video-file.mp4" type="video/mp4">
        
  

    
    <iframe width="600" height="400" src="https://www.youtube.com/embed/your-youtube-video-id" frameborder="0" allowfullscreen></iframe>

  
    <p>Visit our favorite website: <a href="https://www.example.com" target="_blank">Example Website</a></p>

